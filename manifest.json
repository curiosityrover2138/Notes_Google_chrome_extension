{
  "version": "1.2.1",
  "name": "Notes",
  "description": "Create new notes",
  "manifest_version": 2,
  "icons": {
    "16":"icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "page": "src/background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Notes"
  },
  "options_page": "src/options.html"
}
